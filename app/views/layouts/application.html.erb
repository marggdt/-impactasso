<!DOCTYPE html>
<html>
  <head>
    <title>Impactasso</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= stylesheet_pack_tag 'application' %>
    <%= favicon_link_tag %>

    <meta property="og:title" content="Impactasso" />
    <meta property="og:description" content="La solution pour trouver facilement des associations et des missions de bénévolat qui te ressemblent, autour de chez toi!" />
    <meta property="og:image" content="http://www.impactasso.xyz/logo.jpg" />
    <meta property="og:type" content="siteweb" />
    <meta property="og:url" content="http://www.impactasso.xyz" />
    <meta property="fb:app_id" content="59740542" />
    <meta property="og:image:width" content="1200" />
    <meta property="og:image:height" content="630" />


    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>

    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload', defer: true %>
  </head>

    <body>
    <%= render 'shared/navbar' %>
    <%= render 'shared/flashes' %>
      <%= yield %>
     <%= render 'shared/footer' %>

  </body>

</html>
